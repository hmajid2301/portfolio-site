{
  "presets": ["@babel/env", "@babel/react", "@babel/typescript"],
  "plugins": [
    ["transform-class-properties"],
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src"
        }
      }
    ],
    [
      "babel-plugin-import",
      {
        "libraryName": "@material-ui/core",
        "libraryDirectory": "esm",
        "camel2DashComponentName": false
      },
      "core"
    ],
    [
      "babel-plugin-import",
      {
        "libraryName": "@material-ui/icons",
        "libraryDirectory": "esm",
        "camel2DashComponentName": false
      },
      "icons"
    ]
  ]
}

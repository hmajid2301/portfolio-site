  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "coverage:integration": "test:integration:ci && report:coverage",
    "coverage:unit": "jest --coverage",
    "cy:open": "cypress open",
    "cy:open:e2e": "cypress open --config-file cypress-e2e.json",
    "cy:run": "cypress run",
    "cy:run:e2e": "cypress run --config-file cypress-e2e.json",
    "cy:open:smoke": "cypress open --config-file cypress-smoke.json --browser chromium",
    "cy:run:smoke": "cypress run --config-file cypress-smoke.json",
    "debug": "yarn clean && node --nolazy --inspect-brk node_modules/.bin/gatsby develop",
    "deploy": "netlify deploy --dir public/ --prod",
    "develop": "yarn clean && node --max-http-header-size=16385 node_modules/.bin/gatsby develop",
    "format": "prettier src/**/*.{js,jsx,ts,tx,tsx} --write",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tx,tsx}",
    "lint": "eslint --color --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "report:coverage": "nyc report --reporter=text-summary",
    "serve": "node --max-http-header-size=16385 node_modules/.bin/gatsby serve",
    "storybook": "NODE_ENV=production start-storybook",
    "storybook:build": "NODE_ENV=production build-storybook -c .storybook -o .out",
    "test:unit": "jest",
    "test:integration:ci": "start-server-and-test 'yarn develop' http://localhost:8000 'yarn cy:run'",
    "test:e2e:ci": "start-server-and-test 'yarn develop' http://localhost:8000 'yarn cy:run:e2e'",
    "test:e2e:smoke:ci": "start-server-and-test 'yarn develop' http://localhost:8000 'yarn cy:run:smoke'",
    "test:watch": "jest --watch",
    "types:check": "tsc --project . --noEmit --pretty --skipLibCheck"
  },

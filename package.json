{
  "name": "portfolio",
  "private": true,
  "description": "Haseeb Majid's portfolio website.",
  "version": "3.0.0",
  "author": "Haseeb Majid <hello@haseebmajid.dev",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "check": "prettier --check \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "npm run develop",
    "types-check": "tsc --project . --noEmit --pretty --skipLibCheck",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "2.0.4"
  }
}